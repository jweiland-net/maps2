<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:sheetGeneral</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.mapWidth>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapWidth</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <default>100%</default>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.mapWidth>
                    <settings.mapHeight>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapHeight</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <default>300</default>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.mapHeight>
                </el>
            </ROOT>
        </sDEF>
        <sMapOptions>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:sheetMapOptions</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.zoom>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:zoom</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                                <range>
                                    <lower>1</lower>
                                    <upper>20</upper>
                                </range>
                                <wizards>
                                    <zoom>
                                        <type>slider</type>
                                        <step>1</step>
                                        <width>150</width>
                                    </zoom>
                                </wizards>
                                <default>12</default>
                            </config>
                        </TCEforms>
                    </settings.zoom>
                    <settings.forceZoom>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:forceZoom</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.forceZoom>
                    <settings.zoomControl>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:zoomControl</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.zoomControl>
                    <settings.activateScrollWheel>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:activateScrollWheel</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.activateScrollWheel>
                    <settings.fullScreenControl>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:fullScreenControl</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.fullScreenControl>
                </el>
            </ROOT>
        </sMapOptions>
        <sGoogleMapsOptions>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:sheetGoogleMapsOptions</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.mapTypeId>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTypeId</label>
                            <config>
                                <type>radio</type>
                                <default>google.maps.MapTypeId.ROADMAP</default>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTypeId.Hybrid</label>
                                        <value>google.maps.MapTypeId.HYBRID</value>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTypeId.RoadMap</label>
                                        <value>google.maps.MapTypeId.ROADMAP</value>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTypeId.Satellite</label>
                                        <value>google.maps.MapTypeId.SATELLITE</value>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTypeId.Terrain</label>
                                        <value>google.maps.MapTypeId.TERRAIN</value>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.mapTypeId>
                    <settings.mapTypeControl>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTypeControl</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.mapTypeControl>
                    <settings.scaleControl>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:scaleControl</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.scaleControl>
                    <settings.streetViewControl>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:streetViewControl</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.streetViewControl>
                    <settings.styles>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:styles</label>
                            <config>
                                <type>text</type>
                                <rows>10</rows>
                                <cols>50</cols>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.styles>
                </el>
            </ROOT>
        </sGoogleMapsOptions>
        <sOpenStreetMapOptions>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:sheetOpenStreetMapOptions</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.mapTile>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTile</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.mapTile>
                    <settings.mapTileAttribution>
                        <TCEforms>
                            <label>LLL:EXT:maps2/Resources/Private/Language/FlexForms.xlf:mapTileAttribution</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.mapTileAttribution>
                </el>
            </ROOT>
        </sOpenStreetMapOptions>
    </sheets>
</T3DataStructure>
